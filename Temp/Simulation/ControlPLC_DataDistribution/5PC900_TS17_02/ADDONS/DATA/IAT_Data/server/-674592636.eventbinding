						<Action>
							<Target type="opcUa.NodeAction" refId="&quot;urn:B&amp;R/Diagnosis/mappCockpit&quot;|String|SfDomVis.OPC_LoginPassword">
								<Method type="opcUa.NodeAction.SetValueString"><Parameter name="value" xsi:type="ANY_STRING" value="=OperandLoginPassword" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" /></Method>
							</Target>
