  <Content id="SfMP171_contentLogin">
    <EventBinding id="SfMP171_contentLogin.LoginButton.LoginSuccess">
			<Source type="widgets.brease.LoginButton.Event" event="LoginSuccess" refId="SfMP171_contentLogin_SfMP171_btnLogin" />
			<EventHandler>
				<Sequence>
					<Step order="0">
						<Action>
							<Target type="widgets.brease.TextInput.Action" refId="SfMP171_contentLogin_SfMP171_inputLoginUsername">
									<Method type="widgets.brease.TextInput.Action.SetValue"><Parameter name="value" xsi:type="ANY_STRING" value="" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" /></Method>
							</Target>
						</Action>
					</Step>
					<Step order="1">
						<Action>
							<Target type="widgets.brease.Password.Action" refId="SfMP171_contentLogin_SfMP171_inputLoginPassword">
								<Method type="widgets.brease.Password.Action.SetValue"><Parameter name="value" xsi:type="ANY_STRING" value="" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" /></Method>
							</Target>
						</Action>
					</Step>
					<Step order="2">
						<Action>
							<Target type="clientSystem.Action">
								<Method type="clientSystem.Action.CloseDialog"><Parameter name="dialogId" xsi:type="ANY_STRING" value="SfMP193_dialogLogindialog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" /></Method>
							</Target>
						</Action>
					</Step>
				</Sequence>
			</EventHandler>
		</EventBinding>
  </Content>
  <Content id="SfMP171_contentLogin">
    <EventBinding id="SfMP171_contentLogin.LoginButton.AuthentificationFailed">
